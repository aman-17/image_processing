        <parameter name="provider" type="dialog.String" description="Provider of the news"/>
        <parameter name="alternativeProviders" type="dialog.String[]" description="Alternatives of providers"/>
        <parameter name="numberOfAlternativeProviders" type="dialog.Number" description="Number of alternative providers"/>
        <parameter name="device" type="sirikit.DeviceState" description="This still needs support from SKPlatform."/>
        <parameter name="wha" type="audio.WholeHouseAudio" description="WHA Params"/>
    </parameters>
</cat>
