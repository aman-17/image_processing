<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat xmlns="urn:apple:names:siri:cat:1.0" id="PlayMediaIntent#PlaySomethingNew" locale="en">
    <conditions>
        <condition name="attributionWithFallbackUsername" note="attribution required with a fallback user">
            <and>
                <expression name="isAppAttributionRequired"/>
                <expression name="fallbackUsername" op="nonempty"/>
            </and>
        </condition>
        <condition name="attributionWithDialogMemory" note="attribution required using dialog memory">
            <and>
                <expression name="isAppAttributionRequired"/>
                <expression name="useDialogMemoryForAppAttribution"/>
            </and>
        </condition>
        <condition name="notPersonalized">
            <not>
                <expression name="isPersonalized"/>
            </not>
        </condition>
        <condition name="app">
            <and>
                <expression name="app.localizedName" op="nonempty"/>
                <or>
                    <expression name="isAppAttributionRequired"/>
                    <expression name="isAppSpecified"/>
                </or>
            </and>
        </condition>
        <condition name="next">
            <expression name="isPlaybackQueueLocationNext"/>
        </condition>
        <condition name="later">
            <expression name="isPlaybackQueueLocationLater"/>
        </condition>
        <condition name="nextApp" note="This can be removed when phrases work correctly">
            <and>
                <expression name="isPlaybackQueueLocationNext"/>
                <expression name="app.localizedName" op="nonempty"/>
                <or>
                    <expression name="isAppAttributionRequired"/>
                    <expression name="isAppSpecified"/>
                </or>
            </and>
        </condition>
        <condition name="laterApp" note="This can be removed when phrases work correctly">
            <and>
                <expression name="isPlaybackQueueLocationLater"/>
                <expression name="app.localizedName" op="nonempty"/>
                <or>
                    <expression name="isAppAttributionRequired"/>
                    <expression name="isAppSpecified"/>
                </or>
            </and>
        </condition>
        <condition name="areSpeakerNamesPresent">
            <or>
                <expression name="wha.firstSpeakerName"/>
                <expression name="wha.secondSpeakerName"/>
            </or>
        </condition>
        <condition name="areSpeakerRoomsPresent">
            <or>
                <expression name="wha.firstSpeakerRoom"/>
                <expression name="wha.secondSpeakerRoom"/>
            </or>
        </condition>
        <condition name="areBothPermanentNamesPresent">
            <and>
                <expression name="wha.firstPermanentNameFromSpokenEntities"/>
                <expression name="wha.secondPermanentNameFromSpokenEntities"/>
            </and>
        </condition>
        <condition name="isPermanentNameAndRoomNamePresent">
            <and>
                <expression name="wha.firstPermanentNameFromSpokenEntities"/>
                <expression name="wha.firstRoomNameFromSpokenEntities"/>
                <or>
                    <not>
                        <expression name="wha.secondPermanentNameFromSpokenEntities"/>
                    </not>
                    <expression name="wha.secondPermanentNameFromSpokenEntities" op="empty"/>
                </or>
            </and>
        </condition>
        <condition name="isOnlyFirstPermanentNamePresent">
            <and>
                <expression name="wha.firstPermanentNameFromSpokenEntities"/>
                <or>
                    <not>
                        <expression name="wha.secondPermanentNameFromSpokenEntities"/>
                    </not>
                    <expression name="wha.secondPermanentNameFromSpokenEntities" op="empty"/>
                </or>
                <or>
                    <not>
                        <expression name="wha.firstRoomNameFromSpokenEntities"/>
                    </not>
                    <expression name="wha.firstRoomNameFromSpokenEntities" op="empty"/>
                </or>
            </and>
        </condition>
    </conditions>
    <phrases>
        <first phrase="appSpecified">
            <text condition="attributionWithFallbackUsername"><opt><var name="fallbackUsername" inflect="genitive"/> <var name="app.localizedName"/></opt></text>
            <text condition="attributionWithDialogMemory"><opt><var name="app.localizedName" ground="OnceThisWeek"/></opt></text>
            <text condition="app"><opt><var name="app.localizedName"/></opt></text>
        </first>
        <first phrase="MUI" note="Multi User Introduction">
            <text condition="isGrounding">OK, <var name="user.groundingIfNeeded"/>, </text>
            <text></text>
        </first>
        <first phrase="OKMUI" note="OK Multi User Introduction">
            <text condition="isGrounding">OK, <var name="user.groundingIfNeeded"/>, </text>
            <text>OK, </text>
        </first>
        <first phrase="title">
            <text>
                <print>‘<var name="title"/>’</print>
                <speak><say-as interpret-as="music"><var name="title"/></say-as></speak>
            </text>
        </first>
        <first phrase="groupsDialog">
            <text condition="areBothPermanentNamesPresent"> on the <var name="wha.firstPermanentNameFromSpokenEntities"/> and <var name="wha.secondPermanentNameFromSpokenEntities"/></text>
            <text condition="isPermanentNameAndRoomNamePresent"> on the <var name="wha.firstPermanentNameFromSpokenEntities"/> and in the <var name="wha.firstRoomNameFromSpokenEntities"/></text>
            <text condition="isOnlyFirstPermanentNamePresent"> on the <var name="wha.firstPermanentNameFromSpokenEntities"/></text>
            <text></text>
        </first>
        <first phrase="whaDialog">
            <text condition="wha.isAllSpeakers"> everywhere</text>
            <text condition="wha.hasThreeOrMoreRooms"> in multiple rooms</text>
            <text condition="wha.hasGroups"><phrase name="groupsDialog"/></text>
            <text condition="areSpeakerNamesPresent"> on the <var name="wha.firstSpeakerName"/><opt> and <var name="wha.secondSpeakerName"/></opt></text>
            <text condition="areSpeakerRoomsPresent"> in the <var name="wha.firstSpeakerRoom"/><opt> and <var name="wha.secondSpeakerRoom"/></opt></text>
            <text></text>
        </first>
    </phrases>
    <first>
        <random condition="nextApp">
            <random condition="notPersonalized">
                <dialog><phrase name="OKMUI"/><phrase name="title"/> is coming up next<opt> on <phrase name="appSpecified"/></opt>…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> is next in your<opt> <var name="app.localizedName"/></opt> queue…</dialog>
            </random>
            <random condition="isPersonalized">
                <dialog condition="isPlaylist"><phrase name="OKMUI"/><phrase name="title"/> is coming up next<opt> on <phrase name="appSpecified"/></opt>…</dialog>
                <dialog condition="isPlaylist"><phrase name="MUI"/><transform mode="capitalizeSentence">your</transform> personalized <phrase name="title"/> is next in your<opt> <var name="app.localizedName"/></opt> queue…</dialog>
                <dialog condition="isRadio"><phrase name="OKMUI"/><phrase name="title"/> is coming up next<opt> on <phrase name="appSpecified"/></opt>…</dialog>
                <dialog condition="isRadio"><phrase name="MUI"/><phrase name="title"/> is next in your<opt> <var name="app.localizedName"/></opt> queue…</dialog>
            </random>
        </random>
        <random condition="next">
            <random condition="notPersonalized">
                <dialog><phrase name="OKMUI"/><phrase name="title"/> is coming up next…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> is next in your queue…</dialog>
            </random>
            <random condition="isPersonalized">
                <dialog condition="isPlaylist"><phrase name="OKMUI"/><phrase name="title"/> is coming up next…</dialog>
                <dialog condition="isPlaylist"><phrase name="MUI"/><transform mode="capitalizeSentence">your</transform> personalized <phrase name="title"/> is next in your queue…</dialog>
                <dialog condition="isRadio"><phrase name="OKMUI"/><phrase name="title"/> is coming up next…</dialog>
                <dialog condition="isRadio"><phrase name="MUI"/><phrase name="title"/> is next in your queue…</dialog>
            </random>
        </random>
        <random condition="laterApp">
            <random condition="notPersonalized">
                <dialog><phrase name="OKMUI"/><phrase name="title"/> is now in your<opt> <var name="app.localizedName"/></opt> queue…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> is coming up later<opt> on <phrase name="appSpecified"/></opt>…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> is coming up soon<opt> on <phrase name="appSpecified"/></opt>…</dialog>
            </random>
            <random condition="isPersonalized">
                <dialog condition="isPlaylist"><phrase name="OKMUI"/>your personalized <phrase name="title"/> is now added to your<opt> <var name="app.localizedName"/></opt> queue…</dialog>
                <dialog condition="isPlaylist"><phrase name="MUI"/><transform mode="capitalizeSentence">your</transform> personalized <phrase name="title"/> is coming up soon<opt> on <phrase name="appSpecified"/></opt>…</dialog>
                <dialog condition="isPlaylist"><phrase name="MUI"/><transform mode="capitalizeSentence">your</transform> personalized <phrase name="title"/> is coming up later<opt> on <phrase name="appSpecified"/></opt>…</dialog>
                <dialog condition="isRadio"><phrase name="OKMUI"/><phrase name="title"/> is now in your<opt> <var name="app.localizedName"/></opt> queue…</dialog>
                <dialog condition="isRadio"><phrase name="MUI"/><phrase name="title"/> is coming up later<opt> on <phrase name="appSpecified"/></opt>…</dialog>
                <dialog condition="isRadio"><phrase name="MUI"/><phrase name="title"/> is coming up soon<opt> on <phrase name="appSpecified"/></opt>…</dialog>
            </random>
        </random>
        <random condition="later">
            <random condition="notPersonalized">
                <dialog><phrase name="OKMUI"/><phrase name="title"/> is now in your queue…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> is coming up later…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> is coming up soon…</dialog>
            </random>
            <random condition="isPersonalized">
                <dialog condition="isPlaylist"><phrase name="OKMUI"/>your personalized <phrase name="title"/> is now added to your queue…</dialog>
                <dialog condition="isPlaylist"><phrase name="MUI"/><transform mode="capitalizeSentence">your</transform> personalized <phrase name="title"/> is coming up soon…</dialog>
                <dialog condition="isPlaylist"><phrase name="MUI"/><transform mode="capitalizeSentence">your</transform> personalized <phrase name="title"/> is coming up later…</dialog>
                <dialog condition="isRadio"><phrase name="OKMUI"/><phrase name="title"/> is now in your queue…</dialog>
                <dialog condition="isRadio"><phrase name="MUI"/><phrase name="title"/> is coming up later…</dialog>
                <dialog condition="isRadio"><phrase name="MUI"/><phrase name="title"/> is coming up soon…</dialog>
            </random>
        </random>
        <random condition="app">
            <random condition="notPersonalized">
                <dialog><phrase name="MUI"/><transform mode="capitalizeSentence">now</transform> playing <phrase name="title"/><opt> on <phrase name="appSpecified"/></opt><phrase name="whaDialog"/>…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> now playing<opt> on <phrase name="appSpecified"/></opt><phrase name="whaDialog"/>…</dialog>
                <dialog><phrase name="OKMUI"/>here’s <phrase name="title"/><opt> on <phrase name="appSpecified"/></opt><phrase name="whaDialog"/>…</dialog>
            </random>
            <random condition="isPersonalized">
                <dialog><phrase name="MUI"/><transform mode="capitalizeSentence">now</transform> playing <phrase name="title"/>, picked just for you<opt> by <phrase name="appSpecified"/></opt><phrase name="whaDialog"/>…</dialog>
                <dialog><phrase name="MUI"/><phrase name="title"/> now playing, personalized<opt> by <phrase name="appSpecified"/></opt><phrase name="whaDialog"/>…</dialog>
                <dialog><phrase name="OKMUI"/>here’s <phrase name="title"/>, personalized for you<opt> by <phrase name="appSpecified"/></opt><phrase name="whaDialog"/>…</dialog>
            </random>
        </random>
        <random condition="notPersonalized">
            <dialog><phrase name="MUI"/><transform mode="capitalizeSentence">now</transform> playing <phrase name="title"/><phrase name="whaDialog"/>…</dialog>
            <dialog><phrase name="MUI"/><phrase name="title"/> now playing<phrase name="whaDialog"/>…</dialog>
            <dialog><phrase name="OKMUI"/>here’s <phrase name="title"/><phrase name="whaDialog"/>…</dialog>
        </random>
        <random condition="isPersonalized">
            <dialog><phrase name="MUI"/><transform mode="capitalizeSentence">now</transform> playing your personalized <phrase name="title"/><phrase name="whaDialog"/>…</dialog>
            <dialog><phrase name="MUI"/><phrase name="title"/> now playing, picked just for you<phrase name="whaDialog"/>…</dialog>
            <dialog><phrase name="OKMUI"/>here’s <phrase name="title"/>, picked just for you<phrase name="whaDialog"/>…</dialog>
        </random>
    </first>
</cat>
