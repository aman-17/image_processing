            <expression name="requestedRelationship" op="nonempty"/>
        </condition>
    </conditions>
    <first>
        <dialog id="DisambiguateRelationship" condition="isRelationshipRequest"><opt>Ok, <var name="user.groundingIfNeeded"/>. </opt>Which <var name="requestedRelationship"/>?</dialog>
