<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat xmlns="urn:apple:names:siri:cat:1.5" id="SendMessage#ErrorWithCodeMessageLocationNotAllowed" locale="en">
    
    <phrases>
        <first phrase="appName">
            <text condition="app.name"><var name="app.name"/></text>
            <text>The app</text>
        </first>
    </phrases>
    
    <all>
        <dialog><phrase name="appName"/> doesn't have access to your location yet. Open the app to enable Location Services.</dialog>
    </all>
    
</cat>
