<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat xmlns="urn:apple:names:siri:cat:1.5" id="SendMessage#UnsupportedRecipientNoHandleForLabel" locale="en">
    
    <phrases>
        <first phrase="handleType">
            <text condition="recipient.personHandle.formattedType"><var name="recipient.personHandle.formattedType"/></text>
            <text>contact method</text>
        </first>
        
        <first phrase="recipientName">
            <text condition="recipient"><var name="recipient"/></text>
            <text condition="recipient.personHandle.formattedLabel">the recipient</text>
            <text>The recipient</text>
        </first>
    </phrases>
    
    <first>
        <dialog condition="recipient.personHandle.formattedLabel">
            <opt>Sorry, <var name="user.groundingIfNeeded"/>. </opt>You don’t have a <var name="recipient.personHandle.formattedLabel"/> <phrase name="handleType"/> for <phrase name="recipientName"/>.
        </dialog>
        <dialog>
            <opt>Sorry, <var name="user.groundingIfNeeded"/>. </opt><phrase name="recipientName"/> can’t be contacted using the provided <phrase name="handleType"/> label.
        </dialog>
    </first>
    
</cat>
