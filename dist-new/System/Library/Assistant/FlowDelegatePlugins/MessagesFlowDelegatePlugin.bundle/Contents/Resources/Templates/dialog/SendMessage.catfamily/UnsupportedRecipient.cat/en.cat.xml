<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat xmlns="urn:apple:names:siri:cat:1.5" id="SendMessage#UnsupportedRecipient" locale="en">
    
    <phrases>
        <first phrase="appName">
            <text condition="app.name"><var name="app.name"/></text>
            <text>The app</text>
        </first>
        
        <first phrase="needsGrounding">
            <text><opt>Sorry, <var name="user.groundingIfNeeded"/>. </opt></text>
        </first>
    </phrases>
    
    <first>
        <first condition="app.isDesignedByApple">
            <dialog condition="recipient">
                <phrase name="needsGrounding"/>I don’t see <var name="recipient"/> in your Contacts.
            </dialog>
            <dialog>
                <phrase name="needsGrounding"/>I don’t see that person in your Contacts.
            </dialog>
        </first>
        <dialog condition="recipient">
            <phrase name="needsGrounding"/><phrase name="appName"/> couldn’t find anything for <var name="recipient"/>.
        </dialog>
        <dialog>
            <phrase name="needsGrounding"/><phrase name="appName"/> couldn’t find anything for that person.
        </dialog>
    </first>
    
</cat>
