<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat xmlns="urn:apple:names:siri:cat:1.5" id="SendMessage#UnsupportedGroupName" locale="en">
    
    <phrases>
        <first phrase="appName">
            <text condition="app.name"><var name="app.name"/></text>
            <text>The app</text>
        </first>
    </phrases>
    
    <first>
        <dialog condition="groupName">
            <opt>Sorry, <var name="user.groundingIfNeeded"/>. </opt><phrase name="appName"/> doesn’t support the group <var name="groupName"/>.
        </dialog>
        <dialog>
            <opt>Sorry, <var name="user.groundingIfNeeded"/>. </opt><phrase name="appName"/> doesn’t recognize the group.
        </dialog>
    </first>
    
</cat>
