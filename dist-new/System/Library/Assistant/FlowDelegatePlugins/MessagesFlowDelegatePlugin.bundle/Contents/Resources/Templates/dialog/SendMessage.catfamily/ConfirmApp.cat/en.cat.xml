<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat xmlns="urn:apple:names:siri:cat:1.5" id="SendMessage#ConfirmApp" locale="en">
    <phrases>
        <first phrase="appName">
            <text condition="app.name"><var name="app.name"/></text>
            <text>the app</text>
        </first>
    </phrases>
    
    <random>
        <dialog>Would you like to use <phrase name="appName"/>?</dialog>
        <dialog>Great. Confirming you want to use <phrase name="appName"/>?</dialog>
    </random>
</cat>
