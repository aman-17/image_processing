<?xml version="1.0" encoding="UTF-8" standalone="no"?><cat xmlns="urn:apple:names:siri:cat:1.0" id="healthWorkoutPause#intentHandledResponse" locale="en">
  
  <first>
      <first condition="isPunchingOut">
          <dialog condition="appName" id="punchingOutWithAppName">Pausing your workout in <var name="appName"/>…</dialog>
