<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat xmlns="urn:apple:names:siri:cat:1.0" id="runCustomIntent#intentConfirmationPrompt">
    <meta name="description" value="Confirmation prompt."/>
    <meta name="category" value="Confirm"/>
    <meta name="readyForLoc" value="true"/>
    <parameters>
        <parameter name="device" type="sirikit.DeviceState"/>
        <parameter name="intentCategory" type="dialog.String" description="The category of the intent"/>
        <parameter name="categoryVerb" type="dialog.String" description="The verb further describing the category"/>
        <parameter name="localizedAppName" type="dialog.String" description="The localized name of the app"/>
    </parameters>
</cat>

